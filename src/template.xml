<%= htmlWebpackPlugin.tags.headTags.join('\n') %>
<category id="mixly_Sengo1" name="Sengo1视觉传感器" colour="#EF5411">
  <category id="Sengo_Setup" name="设置模块" colour="#EF5411">
    <block type="Sengo1Begin"></block>
    <block type="Sengo1LedSetColor">
      <value name="level">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="Sengo1VisionSetStatus"></block>
    <block type="Sengo1VisionColorSetParam">
        <value name="x">
          <shadow type="math_number">
            <field name="NUM">50</field>
          </shadow>
        </value>
        <value name="y">
          <shadow type="math_number">
            <field name="NUM">50</field>
          </shadow>
        </value>
        <value name="w">
          <shadow type="math_number">
            <field name="NUM">3</field>
          </shadow>
        </value>
        <value name="h">
          <shadow type="math_number">
            <field name="NUM">4</field>
          </shadow>
        </value>
      </block>
    <block type="Sengo1VisionBlobSetParam">
        <value name="w">
          <shadow type="math_number">
            <field name="NUM">3</field>
          </shadow>
        </value>
        <value name="h">
          <shadow type="math_number">
            <field name="NUM">4</field>
          </shadow>
        </value>
      <field name="lable">Sengo1::kColorRed</field>
    </block>
    <block type="Sengo1VisionFaceSetParam">
      <value name="index">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>    
    </block>
  </category>
  <category id="Sengo_Run" name="运行模块" colour="#EAA20A">
    <block type="Sengo1VisionDetectedCount"></block>
    <block type="Sengo1VisionObjColor"></block>
    <block type="Sengo1VisionObj"></block>
    <block type="Sengo1VisionObjLine"></block>
    <block type="Sengo1VisionObjQr"></block>
    <block type="Sengo1GetQrValue"></block>
    <block type="Sengo1VisionColor"></block>
    <block type="Sengo1VisionCardBlob">
      <field name="vision_card_obj">Sengo1::kColorRed</field>
    </block>
    <block type="Sengo1VisionBall"></block>
    <block type="Sengo1VisionCard"></block>
  </category>
  <category id="Sengo_example" name="算法示例" colour="#EAA20A">
    <category id="Sengo_example_Color" name="颜色识别" colour="#EAA20A">
      <block type="base_setup">
        <statement name="DO">
          <block type="Sengo1Begin">
            <field name="mode_obj">Wire</field>
            <field name="addr_obj">0x60</field>
            <next>
              <block type="Sengo1VisionSetStatus">
                <field name="VisionStatus">Begin</field>
                <field name="vision_obj">Sengo1::kVisionColor</field>
                <next>
                  <block type="Sengo1VisionColorSetParam">
                    <field name="vision_obj">Sengo1::kVisionColor</field>
                    <field name="x">50</field>
                    <field name="y">50</field>
                    <field name="w">3</field>
                    <field name="h">4</field>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </statement>
      </block>
      <block type="controls_if">
        <value name="IF0">
          <block type="Sengo1VisionDetectedCount">
            <field name="vision_obj">Sengo1::kVisionColor</field>
          </block>
        </value>
        <statement name="DO0">
          <block type="serial_print">
            <field name="serial_select">Serial</field>
            <field name="new_line">print</field>
            <value name="CONTENT">
              <block type="text_join">
                <value name="A">
                  <shadow type="text">
                    <field name="TEXT">r=</field>
                  </shadow>
                </value>
                <value name="B">
                  <shadow type="text">
                    <field name="TEXT">Mixly</field>
                  </shadow>
                  <block type="Sengo1VisionObjColor">
                    <field name="vision_obj">Sengo1::kVisionColor</field>
                    <field name="vision_res_obj">kRValue</field>
                  </block>
                </value>
              </block>
            </value>
            <next>
              <block type="serial_print">
                <field name="serial_select">Serial</field>
                <field name="new_line">print</field>
                <value name="CONTENT">
                  <block type="text_join">
                    <value name="A">
                      <shadow type="text">
                        <field name="TEXT"> g=</field>
                      </shadow>
                    </value>
                    <value name="B">
                      <shadow type="text">
                        <field name="TEXT">Mixly</field>
                      </shadow>
                      <block type="Sengo1VisionObjColor">
                        <field name="vision_obj">Sengo1::kVisionColor</field>
                        <field name="vision_res_obj">kGValue</field>
                      </block>
                    </value>
                  </block>
                </value>
                <next>
                  <block type="serial_print">
                    <field name="serial_select">Serial</field>
                    <field name="new_line">print</field>
                    <value name="CONTENT">
                      <block type="text_join">
                        <value name="A">
                          <shadow type="text">
                            <field name="TEXT"> b=</field>
                          </shadow>
                        </value>
                        <value name="B">
                          <shadow type="text">
                            <field name="TEXT">Mixly</field>
                          </shadow>
                          <block type="Sengo1VisionObjColor">
                            <field name="vision_obj">Sengo1::kVisionColor</field>
                            <field name="vision_res_obj">kBValue</field>
                          </block>
